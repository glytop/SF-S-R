    <validationRules>
        <fullName>Close_reason_required_when_lost</fullName>
        <active>true</active>
        <description>A close reason is required when opportunity is closed lost.</description>
        <errorConditionFormula>ISPICKVAL( StageName ,&quot;Closed Lost&quot;) &amp;&amp; ISBLANK(Close_Reason__c)</errorConditionFormula>
        <errorDisplayField>Close_Reason__c</errorDisplayField>
        <errorMessage>You must enter a close reason when opportunity is closed lost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_Of_Discount</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(),  Discount_Percentage__c &gt; 0.1 )</errorConditionFormula>
        <errorMessage>Discount percent cannot exceed 10%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_Without_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISCHANGED( StageName ),  ISBLANK( Amount ) )</errorConditionFormula>
        <errorMessage>You can&apos;t change stage if amount is null or empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_Field_Enforced</fullName>
        <active>true</active>
        <errorConditionFormula>$Setup.BypassValidation__c.BypassVR__c == FALSE &amp;&amp; ISPICKVAL(Type, &quot;&quot;)</errorConditionFormula>
        <errorMessage>Type field cannot be blank</errorMessage>
    </validationRules>
